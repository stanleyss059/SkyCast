{
  "expo": {
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/SkyCast-logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/SkyCast-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#E5E5E5"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.stanleysam059.skycast",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/SkyCast-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.stanleysam059.skycast"
    },
    "web": {
      "favicon": "./assets/SkyCast-logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "a87e2a15-ff23-46b6-9971-61a9b058cf13"
      }
    },
    "plugins": [
      "expo-notifications"
    ]
  }
}
